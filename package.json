{
    "name": "Common Automation Framework",
    "version": "3.0.3",
    "description": "",
    "main": "build-js/src/index.js",
    "scripts": {
        "clean": "rimraf build-js & rimraf allure-results",
        "tests": "cross-env environmentToRun=ORANGES npx playwright test StandardTestSpecFile.spec.ts",
        "updateNaukari": "cross-env environmentToRun=doves npx playwright test NaukariProfileUpdate.spec.ts",
        "shard": "cross-env environmentToRun=ORANGES npx playwright test --project=$(project) --shard=$(shard)",
        "ApiTests": "cross-env environmentToRun=PetStore npx playwright test --project=API",
        "tsc": "tsc",
        "build": "npm run clean & npm run tsc",
        "pretest": "npm run build & mkdirp logs",
        "report": "allure generate allure-results --clean",
        "openreport": "allure open",
        "prettify": "prettier --config \".prettierrc.js\" --write \"./**/*.{ts,tsx,js,jsx,json,yaml,yml,eslintrc,md}\" ",
        "lint": "eslint --ignore-path .eslintignore --ext .js,.ts,.tsx .",
        "lintFix": "eslint --fix src/**/*.ts",
        "record": "npx playwright codegen",
        "playwrightreport": "npx playwright show-report"
    },
    "keywords": [
        "Playwright",
        "Typescript"
    ],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@playwright/test": "^1.50.0",
        "@reportportal/agent-js-playwright": "^5.0.8",
        "@types/fs-extra": "^9.0.13",
        "@types/jasmine": "^2.5.43",
        "@types/node": "^22.7.9",
        "cheerio": "1.0.0-rc.2",
        "cross-env": "7.0.3",
        "dotenv": "16.4.1",
        "eslint": "^8.30.0",
        "eslint-config-standard-with-typescript": "^24.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-n": "^15.6.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-simple-import-sort": "^8.0.0",
        "jasmine-reporters": "^2.2.1",
        "jasmine-spec-reporter": "^4.2.1",
        "prettier": "^2.7.1",
        "reflect-metadata": "^0.2.2",
        "typescript": "^5.6.3"
    },
    "dependencies": {
        "allure-commandline": "^2.20.1",
        "allure-playwright": "^2.0.0-beta.20",
        "cheerio": "^1.0.0-rc.12",
        "csv": "^6.2.0",
        "csv-writer": "^1.6.0",
        "csvtojson": "^2.0.10",
        "edit-json-file": "^1.7.0",
        "file-exists": "^5.0.1",
        "fs-extra": "^10.1.0",
        "husky": "^9.1.6",
        "jsonfile": "^6.1.0",
        "junit-report-merger": "^6.0.2",
        "mssql": "^9.0.1",
        "read-json-file": "^1.0.1",
        "read-text-file": "^1.1.0",
        "test-results-reporter": "^1.0.15",
        "update-json-file": "^1.1.1",
        "winston": "^3.14.2",
        "zod": "^3.25.60"
    }
}
